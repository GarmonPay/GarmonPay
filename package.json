{
  "name": "garmonpay",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "kill-port 3000 3001 3002 || true && echo 'GarmonPay server running safely.' && next dev -p 3000",
    "dev:safe": "kill-port 3000 3001 3002 || true && echo 'GarmonPay server running safely.' && next dev",
    "build": "next build",
    "safe": "rm -rf .next && rm -rf node_modules/.cache && next dev",
    "start": "echo 'GarmonPay server running safely.' && next start -p 3000",
    "start:safe": "kill-port 3000 || true && echo 'GarmonPay server running safely.' && next start -p 3000",
    "dev:5000": "npx next dev -p 5000 -H 0.0.0.0",
    "start:4100": "npx next start -p 4100 -H 0.0.0.0",
    "start:4300": "npx next start -p 4300 -H 0.0.0.0",
    "lint": "npx next lint",
    "test:commissions": "node scripts/test-referral-commissions.mjs"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.8.0",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/supabase-js": "^2.76.1",
    "next": "14.2.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^20.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.18",
    "kill-port": "^2.0.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0"
  }
}
